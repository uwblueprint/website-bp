{
  "name": "website-bp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:tsc",
    "lint:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "lint:eslint": "eslint '**/*.{js,jsx,ts,tsx}' --cache --format stylish",
    "lint:tsc": "tsc --noemit",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "fix:eslint": "eslint '**/*.{js,jsx,ts,tsx}' --format stylish --fix",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "next": "10.2.0",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "next-images": "^1.8.1",
    "postcss": "^8.4.4",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
